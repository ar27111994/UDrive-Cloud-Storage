            </div><!-- /.tab-pane -->
        </div>
    </aside><!-- /.control-sidebar -->
    <!-- Add the sidebar's background. This div must be placed
         immediately after the control sidebar -->
    <div class="control-sidebar-bg"></div>

<script type="text/javascript" language="javascript">
   

      

    $(document).ready(function () {
      
    });
      var pieChartDataSource = [
          { category: 'Word', value:@ViewData["DocxInMb"] },
          { category: 'Excel',value:@ViewData["ExcelInMb"] },
          { category: 'Jpg',value:@ViewData["JpgInMb"] },
          { category: 'Jpeg', value:@ViewData["JpegInMb"] },
           { category: 'mp4', value:@ViewData["mp4InMb"] }


      ];

      $(function () {
        $("#pChart").dxPieChart({
          dataSource: pieChartDataSource,
          series: {
            argumentField: 'category',
            valueField: 'value',
            label: {
                visible: true,

              connector: {
                visible: true
              }
            }
          },
          pathModified: true,
          tooltip: {
            enabled: true,
            percentPrecision: 2,
            customizeTooltip: function (value) {
              return {
                text: value.percentText
              };
            }
          },
          title: {
            text: 'Storage by Extention'
          },
          legend: {
              horizontalAlignment: 'center',

            verticalAlignment: 'bottom'
          }
        });
      });

    //online user
    


     // $(function(){
     //     var counterHub=$.connection.counterHub;
     //     $.connection.hub.start().done(function(){
            
            
     //     });
     //     counterHub.client.UpdateCount=function(count){
     //         $("#Counter").text(count);
     //     }
     // });

     $("#package").dxChart({
              dataSource: @Html.Raw(ViewBag.json.ToString()),
            series: [
            { argumentField: "PackageName"}],
            commonSeriesSettings: {
            argumentField: "PackageName",
            type: "bar",
            hoverMode: "allArgumentPoints",
            selectionMode: "allArgumentPoints",
            label: {
            visible: true,
            format: "fixedPoint",
            precision: 0
            }
            },
            series: [
            { valueField: "Price", name: "Price(PKR)" },
            { valueField: "OnlineStorage", name: "Storage(GB)" },
            { valueField: "MoneyBack", name: "MoneyBack(Days)" }



           ],
            title: "UDrive Storage Package With Max Limit",
            legend: {
            verticalAlignment: "bottom",
            horizontalAlignment: "center"
            },
            onPointClick: function (e) {
            e.target.select();
            }
            });


      $("#doughnut").dxPieChart({
          dataSource: pieChartDataSource,
          title: "The Population of Continents and Regions",
          tooltip: {
              enabled: true,
              format:"millions",
              percentPrecision: 2,
              customizeTooltip: function (value) {
                  return {
                      text: value.percentText
                  };
              }
          },
          legend: {
              horizontalAlignment: "right",
              verticalAlignment: "top",
              margin: 0
          },
          series: [{
              argumentField: 'category',
              valueField: 'value',   
              label: {
                  visible: true,
                  format: "millions",
                  connector: {
                      visible: true
                  }
              }
          }]
      });
      $('#Circular').dxCircularGauge({
          scale: {
              startValue: 0,
              endValue: 150,
              tickInterval: 20,
              label: {
                  useRangeColors: true
              }
          },
          rangeContainer: {
              palette: 'pastel',
              ranges: [
                  { startValue: 0, endValue: 50 },
                  { startValue: 50, endValue: 100 },
                  { startValue: 100, endValue: 150 },
              ]
          },

          title: {
              text: 'UDrive Server Storage',
              font: { size: 28 }
          },
          value: @ViewData["RootInGB"]
        
        
      });
    
</script>